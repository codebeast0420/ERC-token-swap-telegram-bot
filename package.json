{
	"name": "chart_ai_bot",
	"version": "1.0.0",
	"description": "ChartAi Bot Telegram",
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"build": "npx tsc",
		"encrypt": "npx tsc && node build/temp/procedures/encryption.js",
		"start": "node --max-old-space-size=49152 build/src/index.js",
		"prod": "rm -rf ../app/build/* && cp -rf ./build/src/* ../app/build/",
		"daemon": "nohup node build/index.js >>bot.log 2>&1 &",
		"kill": "kill -2 $(sudo lsof -t -i:13306)",
		"dev": "NODE_ENV=development && concurrently \"npx tsc -w\" \"nodemon build/src/index.js\"",
		"dev-win": "set NODE_ENV=development && concurrently \"npx tsc -w\" \"nodemon build/src/index.js\"",
		"dev-mac": "NODE_ENV=development && concurrently \"npx tsc -w\"  \"nodemon build/src/index.js\"",
		"dev-ec2": "NODE_ENV=staging && pm2 start build/src/index.js --name backend",
		"start:dev": "npx nodemon build/src/index.js",
		"zipkin": "docker compose up -d",
		"zipkin-logs": "docker logs zipkin"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
